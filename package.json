{
  "name": "iblog",
  "version": "1.0.0",
  "description": "iblog: A theme for Hugo Sites",
  "main": "index.js",
  "scripts": {
    "dev:css": "npx tailwindcss -i assets/css/tailwind.css -o assets/css/gen.css --watch",
    "build:css": "npx tailwindcss -i assets/css/tailwind.css -o assets/css/gen.css --minify",
    "dev:hugo": "hugo server -D -F -s exampleSite --themesDir=../..",
    "dev": "run-p dev:css dev:hugo"
  },
  "keywords": [
    "hugo",
    "gohugo",
    "hugo-theme",
    "gohugo-theme",
    "hugo-module"
  ],
  "author": "Leo (https://github.com/ifuryst)",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.16"
  }
}
